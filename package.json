      "@skyux/action-bars": "^5.0.0-beta.0",
      "@skyux/ag-grid": "^5.0.0-beta.0",
      "@skyux/angular-tree-component": "^5.0.0-beta.0",
      "@skyux/animations": "^5.0.0-beta.0",
      "@skyux/assets": "^5.0.0-beta.0",
      "@skyux/autonumeric": "^5.0.0-beta.0",
      "@skyux/avatar": "^5.0.0-beta.0",
      "@skyux/colorpicker": "^5.0.0-beta.0",
      "@skyux/config": "^5.0.0-beta.0",
      "@skyux/core": "^5.0.0-beta.0",
      "@skyux/data-manager": "^5.0.0-beta.0",
      "@skyux/datetime": "^5.0.0-beta.0",
      "@skyux/docs-tools": "^5.0.0-beta.0",
      "@skyux/e2e-client": "^5.0.0",
      "@skyux/errors": "^5.0.0-beta.0",
      "@skyux/flyout": "^5.0.0-beta.0",
      "@skyux/forms": "^5.0.0-beta.0",
      "@skyux/grids": "^5.0.0-beta.0",
      "@skyux/http": "^5.0.0-beta.0",
      "@skyux/i18n": "^5.0.0-beta.0",
      "@skyux/icons": "^5.0.0-beta.0",
      "@skyux/indicators": "^5.0.0-beta.0",
      "@skyux/inline-form": "^5.0.0-beta.0",
      "@skyux/layout": "^5.0.0-beta.0",
      "@skyux/list-builder": "^5.0.0-beta.0",
      "@skyux/list-builder-common": "^5.0.0-beta.0",
      "@skyux/list-builder-view-checklist": "^5.0.0-beta.0",
      "@skyux/list-builder-view-grids": "^5.0.0-beta.0",
      "@skyux/lists": "^5.0.0-beta.0",
      "@skyux/lookup": "^5.0.0-beta.0",
      "@skyux/modals": "^5.0.0-beta.0",
      "@skyux/navbar": "^5.0.0-beta.0",
      "@skyux/omnibar-interop": "^5.0.0-beta.0",
      "@skyux/pages": "^5.0.0-beta.0",
      "@skyux/phone-field": "^5.0.0-beta.0",
      "@skyux/popovers": "^5.0.0-beta.0",
      "@skyux/progress-indicator": "^5.0.0-beta.0",
      "@skyux/router": "^5.0.0-beta.0",
      "@skyux/select-field": "^5.0.0-beta.0",
      "@skyux/split-view": "^5.0.0-beta.0",
      "@skyux/tabs": "^5.0.0-beta.0",
      "@skyux/text-editor": "^5.0.0-beta.0",
      "@skyux/theme": "^5.0.0-beta.0",
      "@skyux/tiles": "^5.0.0-beta.0",
      "@skyux/toast": "^5.0.0-beta.0",
      "@skyux/validation": "^5.0.0-beta.0"
    }
  },
  "dependencies": {
    "latest-version": "5.1.0",
    "semver": "7.3.5"
  },
  "devDependencies": {
    "@angular-devkit/schematics": "^12.2.5",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@schematics/angular": "^12.2.6",
    "@trivago/prettier-plugin-sort-imports": "^2.0.2",
    "@types/fs-extra": "^9.0.12",
    "@types/jasmine": "^3.6.11",
    "@types/mock-require": "2.0.0",
    "@types/node": "~14.14.45",
    "@types/semver": "7.3.8",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "fs-extra": "10.0.0",
    "jasmine": "^3.6.4",
    "mock-require": "3.0.3",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.2.4"
  }
}
